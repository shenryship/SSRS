    <DataSet Name="ActiveSubscriptions">
      <Query>
        <DataSourceName>Redshift</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!LastDayOfPreviousMonth.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!LastDayOfPreviousMonth.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!LastDayOfPreviousMonth.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!LastDayOfPreviousMonth.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!LastDayOfPreviousMonth.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!LastDayOfPreviousMonth.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	s.CompanyName AS CompanyName,
	trunc(s.CreateDate) AS RegisteredDate,
	MAX(st.AccountName) AS StoreName,  
	CASE
		WHEN COUNT(*) = 1 THEN 0
		ELSE COUNT(*)
	END AS AdditionalStores,
	CASE
		WHEN datediff(day,s.CreateDate,?) &lt;= 30 OR p.billingplanid = 0 THEN 0.00  -- Last day of previous month
		ELSE p.PricePerMonth 
	END AS SubscriptionRevenue,
	CASE
		WHEN datediff(day,s.CreateDate,?) &lt;= 30 OR p.billingplanid = 0 THEN 'Trial'  -- Last day of previous month
		ELSE 'Paid'
	END AS AccountType,
	CASE
		WHEN datediff(day,s.CreateDate,?) &lt;= 30 OR p.billingplanid = 0 THEN 0.00   -- Last day of previous month
		ELSE p.PricePerMonth * 0.25  -- Commission Percent
	END AS AffiliateCommission
FROM
	Seller s
	INNER JOIN Store st ON s.SellerID = st.SellerID
	INNER JOIN BillingPlan p ON s.BillingPlanID = p.BillingPlanID
WHERE
	st.MarketplaceID = 7  -- Volusion  
	AND st.Active = 1
	AND p.Active = 1
	AND s.CreateDate &gt;  '1/7/2013'
	AND trunc(s.CreateDate) &lt;= ?    -- Last day of previous month
	AND (s.CancelDate IS NULL OR (s.CancelDate &gt; add_months(?, -1) AND trunc(s.CancelDate) &lt;= ?))   -- Last day of previous month
	AND st.AccountName NOT LIKE '%shipstation.com'

GROUP BY 
	  s.CompanyName
	, s.CreateDate
	, p.PricePerMonth
	, st.marketplaceid
	, p.billingplanid
ORDER BY 
	s.CreateDate, s.CompanyName</CommandText>
      </Query>
