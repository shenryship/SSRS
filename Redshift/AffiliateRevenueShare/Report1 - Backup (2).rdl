  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="sellerid">
          <DataField>sellerid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="companyname">
          <DataField>companyname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="registereddate">
          <DataField>registereddate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="storename">
          <DataField>storename</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="number_of_additional_stores">
          <DataField>number of additional stores</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="subscription_level">
          <DataField>subscription level</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="accounttype">
          <DataField>accounttype</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="subscriptionrevenue">
          <DataField>subscriptionrevenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Redshift</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT
	s.SellerID+110000 as SellerID, 
	s.CompanyName AS CompanyName,
	s.CreateDate AS RegisteredDate,
	MAX(st.AccountName) AS StoreName,  
	CASE
		WHEN COUNT(1) = 1 THEN ''
		ELSE CONVERT(VARCHAR,COUNT(1)-1)
	END AS "Number of Additional Stores",
	p.PricePerMonth AS "Subscription Level",
	CASE
		WHEN datediff(day,s.CreateDate,trunc(getdate())) &gt; 30 THEN 'Paid'
		ELSE 'Trial'
	END AS "AccountType",
	CASE
		WHEN datediff(day,s.CreateDate,trunc(getdate())) &gt; 30 THEN p.PricePerMonth
		ELSE 0.0
	END AS "SubscriptionRevenue"
	
FROM
	public.Seller s
	INNER JOIN public.Store st ON s.SellerID = st.SellerID
	INNER JOIN public.BillingPlan p ON s.BillingPlanID = p.BillingPlanID
WHERE
	st.MarketplaceID = 13
	AND st.Active = 1
	AND s.CreateDate &gt; '2/28/2013'
	AND s.CreateDate &lt; trunc(getdate())
	AND (s.CancelDate IS NULL OR (s.CancelDate &gt; add_months(trunc(getdate()), -1) AND s.CancelDate &lt; trunc(getdate())))
GROUP BY 
	s.SellerID, s.CompanyName, s.CreateDate, p.PricePerMonth
ORDER BY 
	s.CreateDate, s.CompanyName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
